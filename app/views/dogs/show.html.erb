
<div class="banner" style="background-image: linear-gradient(-225deg, rgba(50,50,50,0.2) 0%, rgba(0,0,0,0.2) 50%), url('https://images.unsplash.com/photo-1455380579765-810023662ea2?ixlib=rb-0.3.5&ixid=eyJhcHBfaWQiOjEyMDd9&s=c588e711c7511c30d1b9341cfdc6691d&auto=format&fit=crop&w=1950&q=80');">
</div>
<div class="container" id="show">
    <div class="row">
      <div class="col-xs-12 col-md-6">
          <div class="details">
            <h4>Details</h4>
            <p>Breed: <%= @dog.breed %></p>
            <p>Skill: <%= @dog.skill %></p>
            <p>Age: <%= @dog.age %></p>
            <p>Fee: <%= @dog.fee %></p>
          </div>
      </div>
      <div class="col-xs-12 col-md-6">
        <%= render 'bookings/form', booking: [@dog, @booking] %>
      </div>
    </div>
</div>
